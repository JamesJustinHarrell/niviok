(global
	/*
	function main() {
		doStuff()
	}
	*/
	(declaration-pervasive
		(identifier main)
		(refence-type function)
		(function.value
			(block
				(function-call
					(identifier.function doStuff)))))

	/*
	function doStuff() {
		if true {
			print("this is a test")
		}
		print( "Hello,".concat(" World") )
		for x from 1 to 20  {
			print( x.add(10000) )
		}
	}
	*/
	(declaration-pervasive
		(identifier doStuff)
		(reference-type function)
		(function.value
			(block
				(conditional-block
					(identifier.test true)
					(block.action
						(function-call
							(identifier.function print)
							(string.argument "this is a test"))))
				(function-call
					(identifier.function print)
					(method-call.argument
						(string.value "Hello,")
						(identifier.method-name concat)
						(string.argument " World")))
				(for-range
					(identifier x)
					(integer.start 1)
					(integer.limit 20)
					(block
						(function-call
							(identifier.function print)
							(method-call.argument
								(identifier.value x)
								(identifier.method-name add)
								(integer.argument 10000)))))))))